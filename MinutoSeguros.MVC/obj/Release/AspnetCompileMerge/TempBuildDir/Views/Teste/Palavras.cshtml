@model IEnumerable<MinutoSeguros.Lib.Class.Palavra>

@{
    ViewBag.Title = "Palavras";
}

<h2>@ViewBag.Title</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Literal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vezes)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Literal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vezes)
        </td>
    </tr>
}

</table>
